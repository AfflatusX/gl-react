    "browserify": "^14.3.0",
    "browserify-shim": "^3.8.12",
    "documentation": "4.0.0-beta.18",
    "flow-bin": "^0.49.1",
    "flow-copy-source": "^1.1.0",
    "lerna": "^2.0.0-rc.5",
    "prettier": "^1.5.2"
